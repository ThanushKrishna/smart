// This is your Prisma schema file,
// learn more about it in the docs https//pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

enum ROLE {
  USER
  ADMIN
}

enum GENDER {
  MALE
  FEMALE
}

type ADDRESS {
  street String?
  city   String?
  state  String?
  zip    String?
}

model app_user {
  userid      String   @id @default(auto()) @map("_id") @db.ObjectId
  emailid     String   @unique
  password    String
  firstname   String?
  lastname    String?
  gender      GENDER?
  address     ADDRESS?
  profile_pic String?
  mobile      String?  @unique
  role        ROLE     @default(USER)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model user_data {
  id                    String    @id @default(auto()) @map("_id") @db.ObjectId
  data_owner_id         String    @db.ObjectId
  Vehicle_No            String    @unique
  RC_No                 String?
  Registered_Date       DateTime?
  Owner                 String?
  Gender                String?
  Owner_dob             DateTime?
  Ownership_type        String?
  Vehicle_type          String?
  Vehicle_Kind          String?
  Year_of_manufacuring  DateTime?
  GVW                   String?
  Chasis_No             String?
  Engine_No             String?
  FC_due_Date           DateTime?
  tax_due_Date          DateTime?
  Vehicle_color         String?
  Vehice_norms          String?
  Address               ADDRESS?
  CAddress              ADDRESS?
  CC                    String?
  Make                  String?
  Model                 String?
  Insurance_provider    String?
  Insurance_dueDate     DateTime?
  Policy_No             String?
  Permit_No             String?
  Permit_category       String?
  Permit_dueDate        DateTime?
  Mobile_No1            String?
  Mobile_No2            String?
  Email_id              String?
  Adhar_No              String?
  Adhar_doc             String?
  PanCard_No            String?
  Pan_doc               String?
  Nominee               String?
  Nominee_dob           DateTime?
  Emission_dueDate      DateTime?
  Fuel_type             String?
  Hypothecation_bank    String?
  Hypothecation_city    String?
  RTO                   String?
  Referred_by           String?
  Comments              String?
  Customer_type         String?
  Martial_status        String?
  TP_Insurance_provider String?
  TP_dueDate            DateTime?
  GST_No                String?
  Insurance_type        String?
  Mobile_No3            String?
  Nominee_Relationship  String?
  Son_Wife_Daughter_Of  String?
  Vehicle_Body          String?
  Wheel_Base            String?
  No_Of_Cylinder        String?
  Unladen_Weight        String?
  Sleeper_Capacity      String?
  PUCC_Emission_No      String?
  updated_by            String?
  TP_Policy_No          String?
  Insurance_Start       DateTime?
  TP_Insurance_Start    DateTime?
  Vehicle_Reg_Doc       String?
  OD_Policy_Doc         String?
  TP_Policy_Doc         String?
  GST_Cer_Doc           String?  
  Vehicle_Description   String?
  Seating_Capacity      String?
  Standing_Capacity     String?
  Prospect              String?
  createdAt             DateTime  @default(now())
  updatedAt             DateTime  @updatedAt
}

model VEHICLE_COLOR {
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  data_owner_id String @db.ObjectId
  value         String @unique
}

